# RRDM Development Environment

# Core Configuration
# -----------------
NODE_ENV=development
PORT=3000
SESSION_SECRET=rrdm-dev-secret-key

# Database Configuration
# ---------------------
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rrdm_local?schema=public
PRISMA_CLIENT_ENGINE_TYPE=binary

# Redis Configuration
# ------------------
REDIS_ENABLED=false
REDIS_MOCK=true
REDIS_URL=redis://localhost:6379

# Logging Configuration
# --------------------
LOG_LEVEL=debug

# Session Configuration
# --------------------
SESSION_PERSISTENCE=true

# Feature Flags
# ------------
ENABLE_DEBUG_ROUTES=true
ENABLE_MOCK_DATA=true

# Security Settings
# ----------------
CSRF_PROTECTION=true
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000
